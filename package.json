{
  "name": "react-typeout",
  "version": "0.6.1",
  "description": "Simply a react component that types by itself.",
  "keywords": [
    "react",
    "typewriter",
    "component"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "node_modules/.bin/rimraf ./lib",
    "build:lib": "node_modules/.bin/babel src/ -d lib/",
    "build": "npm run clean && npm run build:lib && npm run demo:build",
    "lint": "node_modules/.bin/eslint src/",
    "test": "node_modules/.bin/karma start test/karma.conf.js",
    "demo:watch": "webpack-dev-server --inline --config webpack.config.js --content-base docs --port 8721 --host 0.0.0.0",
    "demo:build": "NODE_ENV=production node_modules/.bin/webpack",
    "prepublish": "npm run lint && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.2.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.11.16",
    "enzyme": "^2.7.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "istanbul": "~0.3.17",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^0.5.5",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "1.0.5",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "~1.2.8",
    "sinon": "2.1.0",
    "sinon-chai": "^2.8.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "webpack": "~1.11.0",
    "webpack-dev-server": "~1.10.1"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "author": "Philip Knape <philip.knape@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Knape/react-typeout.git"
  },
  "bugs": {
    "url": "https://github.com/Knape/react-typeout/issues"
  },
  "homepage": "https://knape.github.io/react-typeout",
